<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="products" match-catalog="lior"></table-filter>
	<table-filter match-name="products_in_store" match-catalog="lior"></table-filter>
	<table-filter match-name="products_in_store_prices"
		match-catalog="lior">
	</table-filter>
	<table-filter match-name="stores" match-catalog="lior"></table-filter>
</hibernate-reverse-engineering>